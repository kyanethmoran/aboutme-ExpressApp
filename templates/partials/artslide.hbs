{{!-- //script to run carousel --}}
<script>
var myIndexArt = 0;
carouselArt();

function carouselArt() {
  var i;
  var x = document.getElementsByClassName("artSlides");
  for (i = 0; i < x.length; i++) {
    x[i].style.display = "none";  
  }
  myIndexArt++;
  if (myIndexArt > x.length) {myIndexArt = 1}    
  x[myIndexArt-1].style.display = "block";  
  setTimeout(carouselArt, 9000);    
}
</script>